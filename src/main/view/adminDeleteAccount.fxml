<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="146.0" prefWidth="228.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.AdminController">
   <children>
      <Label fx:id="alert" layoutX="14.0" layoutY="14.0" text="Enter Username to delete">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="14.0" layoutY="48.0" prefHeight="25.0" prefWidth="202.0" promptText="username" />
      <Button fx:id="deleteAccount" layoutX="131.0" layoutY="92.0" mnemonicParsing="false" onAction="#deleteAccount" text="Delete" />
      <Button fx:id="backHome" layoutX="33.0" layoutY="92.0" mnemonicParsing="false" onAction="#backHomeHandler" prefHeight="25.0" prefWidth="51.0" text="Back" />
   </children>
</AnchorPane>
