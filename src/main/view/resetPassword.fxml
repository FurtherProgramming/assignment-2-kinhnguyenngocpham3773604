<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.ResetPasswordController">
   <children>
      <Label fx:id="confirmation" alignment="CENTER" layoutX="75.0" layoutY="14.0" text="Reset Password">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="151.0" layoutY="62.0" />
      <PasswordField fx:id="txtPasswordConfirmation" layoutX="151.0" layoutY="112.0" />
      <Label layoutX="14.0" layoutY="66.0" text="New Password" />
      <Label layoutX="14.0" layoutY="116.0" text="Password Confirmation" />
      <Button fx:id="resetPassword" layoutX="176.0" layoutY="160.0" mnemonicParsing="false" onAction="#ResetPassword" text="Reset Password" />
   </children>
</AnchorPane>
